/*Table structure for table `sec_apps` */

DROP TABLE IF EXISTS `sec_apps`;

CREATE TABLE `sec_apps` (
  `app_name` varchar(128) NOT NULL,
  `app_type` varchar(255) DEFAULT NULL,
  `description` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`app_name`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

/*Table structure for table `sec_groups` */

DROP TABLE IF EXISTS `sec_groups`;

CREATE TABLE `sec_groups` (
  `group_id` int(11) NOT NULL AUTO_INCREMENT,
  `description` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`group_id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

/*Table structure for table `sec_groups_apps` */

DROP TABLE IF EXISTS `sec_groups_apps`;

CREATE TABLE `sec_groups_apps` (
  `group_id` int(11) NOT NULL,
  `app_name` varchar(128) NOT NULL,
  `priv_access` varchar(1) DEFAULT NULL,
  `priv_insert` varchar(1) DEFAULT NULL,
  `priv_delete` varchar(1) DEFAULT NULL,
  `priv_update` varchar(1) DEFAULT NULL,
  `priv_export` varchar(1) DEFAULT NULL,
  `priv_print` varchar(1) DEFAULT NULL,
  PRIMARY KEY (`group_id`,`app_name`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

/*Table structure for table `sec_logged` */

DROP TABLE IF EXISTS `sec_logged`;

CREATE TABLE `sec_logged` (
  `login` varchar(255) NOT NULL,
  `date_login` varchar(128) DEFAULT NULL,
  `sc_session` varchar(128) DEFAULT NULL,
  `ip` varchar(255) DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

/*Table structure for table `sec_settings` */

DROP TABLE IF EXISTS `sec_settings`;

CREATE TABLE `sec_settings` (
  `set_name` varchar(255) NOT NULL,
  `set_value` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`set_name`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

/*Table structure for table `sec_users` */

DROP TABLE IF EXISTS `sec_users`;

CREATE TABLE `sec_users` (
  `login` varchar(255) NOT NULL,
  `pswd` varchar(255) NOT NULL,
  `name` varchar(255) DEFAULT NULL,
  `email` varchar(255) DEFAULT NULL,
  `active` varchar(1) DEFAULT NULL,
  `activation_code` varchar(32) DEFAULT NULL,
  `priv_admin` varchar(1) DEFAULT NULL,
  `mfa` varchar(255) DEFAULT NULL,
  `picture` longblob,
  `role` varchar(128) DEFAULT NULL,
  `phone` varchar(64) DEFAULT NULL,
  `pswd_last_updated` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `mfa_last_updated` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`login`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

/*Table structure for table `sec_users_groups` */

DROP TABLE IF EXISTS `sec_users_groups`;

CREATE TABLE `sec_users_groups` (
  `login` varchar(255) NOT NULL,
  `group_id` int(11) NOT NULL,
  PRIMARY KEY (`login`,`group_id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

/*Table structure for table `tbl_attendance` */

DROP TABLE IF EXISTS `tbl_attendance`;

CREATE TABLE `tbl_attendance` (
  `Auto_No` int(24) DEFAULT NULL,
  `userid` int(30) DEFAULT NULL,
  `Username` varchar(255) DEFAULT NULL,
  `date` date DEFAULT NULL,
  `daytype` varchar(45) DEFAULT NULL,
  `att_in` varchar(5) DEFAULT NULL,
  `att_out` varchar(5) DEFAULT NULL,
  `workhour` decimal(4,2) DEFAULT '0.00',
  `othour` decimal(4,2) DEFAULT '0.00',
  `workhour_hh` decimal(24,2) DEFAULT '0.00',
  `othour_hh` decimal(24,2) DEFAULT '0.00',
  `shorthour` decimal(4,2) DEFAULT '0.00',
  `designation` varchar(100) DEFAULT NULL,
  `path` varchar(255) DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

/*Table structure for table `tbl_attendance_payroll` */

DROP TABLE IF EXISTS `tbl_attendance_payroll`;

CREATE TABLE `tbl_attendance_payroll` (
  `idattendance_sheet` int(30) NOT NULL AUTO_INCREMENT,
  `userid` varchar(30) DEFAULT NULL,
  `userid_int` int(11) DEFAULT NULL,
  `date` date DEFAULT NULL,
  `daytype` varchar(45) DEFAULT NULL,
  `leavetype` int(10) DEFAULT NULL,
  `sche1` int(10) DEFAULT NULL,
  `shiftNo` int(10) DEFAULT NULL,
  `att_in` varchar(5) DEFAULT NULL,
  `att_break` varchar(5) DEFAULT NULL,
  `att_resume` varchar(5) DEFAULT NULL,
  `att_out` varchar(5) DEFAULT NULL,
  `att_ot` varchar(5) DEFAULT NULL,
  `att_done` varchar(5) DEFAULT NULL,
  `workhour` decimal(4,2) DEFAULT NULL,
  `othour` decimal(4,2) DEFAULT NULL,
  `workhour_hh` decimal(24,2) DEFAULT '0.00',
  `othour_hh` decimal(24,2) DEFAULT '0.00',
  `diffothour` decimal(4,2) DEFAULT NULL,
  `shorthour` decimal(4,2) DEFAULT NULL,
  `shorthour_hh` decimal(24,2) DEFAULT '0.00',
  `SumWork` decimal(7,3) DEFAULT NULL,
  `SumOT` decimal(7,3) DEFAULT NULL,
  `SumDiffOT` decimal(7,3) DEFAULT NULL,
  `SumShort` decimal(7,3) DEFAULT NULL,
  `TotHr` decimal(7,3) DEFAULT NULL,
  `TotWork` decimal(7,3) DEFAULT NULL,
  `TotOT` decimal(7,3) DEFAULT NULL,
  `TotDiff` decimal(7,3) DEFAULT NULL,
  `TotDiffOT` decimal(7,3) DEFAULT NULL,
  `BankHour` decimal(7,3) DEFAULT NULL,
  `LeaveHour` decimal(7,3) DEFAULT NULL,
  `in_s` decimal(4,2) DEFAULT NULL,
  `break_s` decimal(4,2) DEFAULT NULL,
  `resume_s` decimal(4,2) DEFAULT NULL,
  `out_s` decimal(4,2) DEFAULT NULL,
  `ot_s` decimal(4,2) DEFAULT NULL,
  `done_s` decimal(4,2) DEFAULT NULL,
  `daytype_c` int(10) DEFAULT NULL,
  `leavetype_c` int(10) DEFAULT NULL,
  `remark_c` int(10) DEFAULT NULL,
  `sche1_c` int(10) DEFAULT NULL,
  `in_c` int(10) DEFAULT NULL,
  `break_c` int(10) DEFAULT NULL,
  `resume_c` int(10) DEFAULT NULL,
  `out_c` int(10) DEFAULT NULL,
  `ot_c` int(10) DEFAULT NULL,
  `done_c` int(10) DEFAULT NULL,
  `workhour_c` int(10) DEFAULT NULL,
  `othour_c` int(10) DEFAULT NULL,
  `diffothour_c` int(10) DEFAULT NULL,
  `shorthour_c` int(10) DEFAULT NULL,
  `SumWork_c` int(10) DEFAULT NULL,
  `SumOT_c` int(10) DEFAULT NULL,
  `SumDiffOT_c` int(10) DEFAULT NULL,
  `SumShort_c` int(10) DEFAULT NULL,
  `TotHr_c` int(10) DEFAULT NULL,
  `TotWork_c` int(10) DEFAULT NULL,
  `TotOT_c` int(10) DEFAULT NULL,
  `TotDiff_c` int(10) DEFAULT NULL,
  `TotDiffOT_c` int(10) DEFAULT NULL,
  `BankHour_c` int(10) DEFAULT NULL,
  `LeaveHour_c` int(10) DEFAULT NULL,
  `in_x` int(10) DEFAULT NULL,
  `break_x` int(10) DEFAULT NULL,
  `resume_x` int(10) DEFAULT NULL,
  `out_x` int(10) DEFAULT NULL,
  `ot_x` int(10) DEFAULT NULL,
  `done_x` int(10) DEFAULT NULL,
  `in_o` varchar(5) DEFAULT NULL,
  `break_o` varchar(5) DEFAULT NULL,
  `resume_o` varchar(5) DEFAULT NULL,
  `out_o` varchar(5) DEFAULT NULL,
  `ot_o` varchar(5) DEFAULT NULL,
  `done_o` varchar(5) DEFAULT NULL,
  `remark` int(10) DEFAULT NULL,
  `hasmisspunch` tinyint(3) unsigned DEFAULT NULL,
  `createdate` timestamp NULL DEFAULT NULL,
  `lastupdate` timestamp NULL DEFAULT NULL,
  `desc` varchar(200) DEFAULT NULL,
  `desc_c` int(10) DEFAULT NULL,
  PRIMARY KEY (`idattendance_sheet`)
) ENGINE=InnoDB AUTO_INCREMENT=3473 DEFAULT CHARSET=utf8;

