/*Table structure for table `tbl_batch_boni` */

DROP TABLE IF EXISTS `tbl_batch_boni`;

CREATE TABLE `tbl_batch_boni` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `boni_code` varchar(255) DEFAULT NULL,
  `boni_description` varchar(255) DEFAULT NULL,
  `date_debut` date DEFAULT NULL,
  `date_fin` date DEFAULT NULL,
  `status` int(11) DEFAULT NULL,
  `created_date` datetime DEFAULT NULL,
  `last_update` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=latin1;

/*Table structure for table `tbl_batch_payroll` */

DROP TABLE IF EXISTS `tbl_batch_payroll`;

CREATE TABLE `tbl_batch_payroll` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `payroll_code` varchar(255) DEFAULT NULL,
  `payroll_description` varchar(255) DEFAULT NULL,
  `date_debut` date DEFAULT NULL,
  `date_fin` date DEFAULT NULL,
  `date_debut_sem1` date DEFAULT NULL,
  `date_fin_sem1` date DEFAULT NULL,
  `date_debut_sem2` date DEFAULT NULL,
  `date_fin_sem2` date DEFAULT NULL,
  `status` int(11) DEFAULT NULL,
  `posted_date` date DEFAULT NULL,
  `created_date` datetime DEFAULT NULL,
  `last_update` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=33 DEFAULT CHARSET=latin1;

/*Table structure for table `tbl_boni` */

DROP TABLE IF EXISTS `tbl_boni`;

CREATE TABLE `tbl_boni` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `userid_varchar` varchar(30) DEFAULT NULL,
  `userid_int` int(11) DEFAULT NULL,
  `hiredate` date DEFAULT NULL,
  `username` varchar(255) DEFAULT NULL,
  `dept` varchar(255) DEFAULT NULL,
  `designation` varchar(255) DEFAULT NULL,
  `Rate_Work` decimal(24,2) DEFAULT '0.00',
  `Rate_OT` decimal(24,2) DEFAULT '0.00',
  `Rate_SALF` decimal(24,2) DEFAULT '0.00',
  `payperiod` int(10) DEFAULT '1',
  `probation_period` int(3) DEFAULT NULL,
  `rate_ot_factor` decimal(24,2) DEFAULT '0.00',
  `rate_ot_holiday_factor` decimal(24,2) DEFAULT '0.00',
  `rate_ot_restday_factor` decimal(24,2) DEFAULT '0.00',
  `work_hh_w` decimal(24,2) DEFAULT '0.00',
  `ot_hh_w` decimal(24,2) DEFAULT '0.00',
  `work_hh_h` decimal(24,2) DEFAULT '0.00',
  `ot_hh_h` decimal(24,2) DEFAULT '0.00',
  `hh_sunday` decimal(24,2) DEFAULT '0.00',
  `work_hh_r` decimal(24,2) DEFAULT '0.00',
  `ot_hh_r` decimal(24,2) DEFAULT '0.00',
  `hh_offday` decimal(24,2) DEFAULT '0.00',
  `work_hh_o` decimal(24,2) DEFAULT '0.00',
  `ot_hh_o` decimal(24,2) DEFAULT '0.00',
  `ot_hh` decimal(24,2) DEFAULT '0.00',
  `ot_value` decimal(24,2) DEFAULT '0.00',
  `short_hh` decimal(24,2) DEFAULT '0.00',
  `short_value` decimal(24,2) DEFAULT '0.00',
  `sal_work` decimal(24,2) DEFAULT '0.00',
  `sal_ot` decimal(24,2) DEFAULT '0.00',
  `sal_sum` decimal(24,2) DEFAULT '0.00',
  `rate_ona` decimal(24,2) DEFAULT '0.00',
  `rate_cass` decimal(24,2) DEFAULT '0.00',
  `rate_cfgdct` decimal(24,2) DEFAULT '0.00',
  `rate_fdu` decimal(24,2) DEFAULT '0.00',
  `rate_iris` decimal(24,2) DEFAULT '0.00',
  `rate_iric` decimal(24,2) DEFAULT '0.00',
