CREATE DATABASE payroll_db'
USE `payroll_db`;

/*Table structure for table `del_tbl_pret` */

DROP TABLE IF EXISTS `del_tbl_pret`;

CREATE TABLE `del_tbl_pret` (
  `id` INT(11) NOT NULL AUTO_INCREMENT,
  `dept` INT(11) DEFAULT NULL,
  `userid_int` INT(11) DEFAULT NULL,
  `userid_varchar` VARCHAR(255) DEFAULT NULL,
  `pret` DECIMAL(24,2) DEFAULT '0.00',
  `date_pret` DATE DEFAULT NULL,
  `deduction` DECIMAL(24,2) DEFAULT '0.00',
  `debut_deduct` DATE DEFAULT NULL,
  `fin_deduct` DATE DEFAULT NULL,
  `apply_deduction` INT(4) DEFAULT '0',
  PRIMARY KEY (`id`)
) ENGINE=INNODB DEFAULT CHARSET=latin1;

/*Table structure for table `del_tbl_salary_historized` */

DROP TABLE IF EXISTS `del_tbl_salary_historized`;

CREATE TABLE `del_tbl_salary_historized` (
  `id` INT(11) NOT NULL AUTO_INCREMENT,
  `userid_varchar` VARCHAR(30) DEFAULT NULL,
  `userid_int` INT(11) DEFAULT NULL,
  `hiredate` date DEFAULT NULL,
  `username` varchar(255) DEFAULT NULL,
  `dept` varchar(255) DEFAULT NULL,
  `designation` varchar(255) DEFAULT NULL,
  `Rate_Work` decimal(24,2) DEFAULT '0.00',
  `Rate_OT` decimal(24,2) DEFAULT '0.00',
  `Rate_SALF` decimal(24,2) DEFAULT '0.00',
  `rate_ot_factor` decimal(24,2) DEFAULT '0.00',
  `rate_ot_holiday_factor` decimal(24,2) DEFAULT '0.00',
  `work_hh_w` decimal(24,2) DEFAULT '0.00',
  `ot_hh_w` decimal(24,2) DEFAULT '0.00',
  `work_hh_h` decimal(24,2) DEFAULT '0.00',
  `ot_hh_h` decimal(24,2) DEFAULT '0.00',
  `hh_sunday` decimal(24,2) DEFAULT '0.00',
  `work_hh_r` decimal(24,2) DEFAULT '0.00',
  `ot_hh_r` decimal(24,2) DEFAULT '0.00',
  `hh_offday` decimal(24,2) DEFAULT '0.00',
  `work_hh_o` decimal(24,2) DEFAULT '0.00',
  `ot_hh_o` decimal(24,2) DEFAULT '0.00',
  `sal_work` decimal(24,2) DEFAULT '0.00',
  `sal_ot` decimal(24,2) DEFAULT '0.00',
  `sal_sum` decimal(24,2) DEFAULT '0.00',
  `rate_ona` decimal(24,2) DEFAULT '0.00',
  `rate_cfgdct` decimal(24,2) DEFAULT '0.00',
  `rate_iris` decimal(24,2) DEFAULT '0.00',
  `rate_iric` decimal(24,2) DEFAULT '0.00',
  `tax_ona` decimal(24,2) DEFAULT '0.00',
  `tax_cfgdct` decimal(24,2) DEFAULT '0.00',
  `tax_iris` decimal(24,2) DEFAULT '0.00',
  `tax_iric` decimal(24,2) DEFAULT '0.00',
  `tax_sum` decimal(24,2) DEFAULT '0.00',
  `deduction_pret` decimal(24,2) DEFAULT '0.00',
  `sal_hr_worked` decimal(24,2) DEFAULT '0.00',
  `sal_sunday` decimal(24,2) DEFAULT '0.00',
  `sal_holiday` decimal(24,2) DEFAULT '0.00',
  `sal_brut_reg` decimal(24,2) DEFAULT '0.00',
  `sal_suppl` decimal(24,2) DEFAULT '0.00',
  `sal_holiday_worked_suppl` decimal(24,2) DEFAULT '0.00',
  `sal_holiday_suppl` decimal(24,2) DEFAULT '0.00',
  `sal_restday_worked_suppl` decimal(24,2) DEFAULT '0.00',
  `sal_restday_suppl` decimal(24,2) DEFAULT '0.00',
  `incentive` decimal(24,2) DEFAULT '0.00',
  `rappel` decimal(24,2) DEFAULT '0.00',
  `day_cons` decimal(24,2) DEFAULT '0.00',
  `rate_cons` decimal(24,2) DEFAULT '0.00',
  `loan_deduction` decimal(24,2) DEFAULT '0.00',
  `loan_deduction_bank` decimal(24,2) DEFAULT '0.00',
  `other_loan_deduction` decimal(24,2) DEFAULT '0.00',
  `dedeuct_cons` decimal(24,2) DEFAULT '0.00',
  `other_deduct` decimal(24,2) DEFAULT '0.00',
  `total_deduct` decimal(24,2) DEFAULT '0.00',
  `sal_net` decimal(24,2) DEFAULT '0.00',
  `bank_number` varchar(255) DEFAULT NULL,
  `id_batch_payroll` int(11) DEFAULT NULL,
  `generated_date` datetime DEFAULT NULL,
  `historized_date` timestamp NULL DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=1078 DEFAULT CHARSET=latin1;

